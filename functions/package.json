{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "npm run lint && cd ../shared && npm run build && cd ../functions && tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "deploy:skipLint": "cd ../shared && npm run build && cd ../functions && tsc && firebase deploy --only functions",
    "logs": "firebase functions:log",
    "gcp-build": "node node_modules/puppeteer/install.js"
  },
  "engines": {
    "node": "20"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@qdrant/js-client-rest": "^1.8.2",
    "@types/jsdom": "^21.1.4",
    "express": "^4.17.1",
    "firebase-admin": "^11.8.0",
    "firebase-functions": "^6.3.2",
    "jsdom": "^22.1.0",
    "json-stringify-deterministic": "^1.0.1",
    "md5": "^2.2.1",
    "nodemailer": "^6.6.1",
    "nodemailer-postmark-transport": "^2.2.0",
    "openai": "^4.86.2",
    "puppeteer": "^18.2.1",
    "stable": "^0.1.8",
    "twitter": "^1.7.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/md5": "^2.3.4",
    "@types/nodemailer": "^6.4.13",
    "@types/twitter": "^1.7.3",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^8.17.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-promise": "^6.1.1",
    "firebase-functions-test": "^3.1.0",
    "typescript": "^4.9.0"
  },
  "private": true
}
